	UpgradedList.changed_list.connect(_check_upgrade)


func _check_upgrade():
	if UpgradedList.upgrade_is_more_level(upgrade_name, 1):
		create_tween().tween_property(self, \"modulate\", Color(1, 1, 1, 1), 1.5)
"

