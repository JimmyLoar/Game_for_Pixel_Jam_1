[sub_resource type="GDScript" id="GDScript_grlqs"]
script/source = "extends MarginContainer

@onready var progress_bar: ProgressBar = $ProgressBar

func _ready() -> void:
	progress_bar.max_value = 65
	UpgradedList.changed_list.connect(_update)
	_update()


func _update():
	var value = UpgradedList.get_level(\"create_part_world\")
	progress_bar.value = value
	$Label.text = TranslationServer.translate(\"SAVE_PROGRESS_TEXT\") % \\
		[(value / float(progress_bar.max_value)) * 100]
"

